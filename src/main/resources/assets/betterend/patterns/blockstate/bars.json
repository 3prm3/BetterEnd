{
	"multipart": [
		{
			"when": {
				"west": "false",
				"east": "false",
				"south": "false",
				"north": "false"
			},
			"apply": {
				"model": "betterend:pattern/%block%/%block%_post"
			}
		},
		{
			"when": {
				"north": "true"
			},
			"apply": {
				"model": "betterend:pattern/%block%/%block%_side"
			}
		},
		{
			"when": {
				"east": "true"
			},
			"apply": {
				"model": "betterend:pattern/%block%/%block%_side",
				"uvlock": true,
				"y": 90
			}
		},
		{
			"when": {
				"south": "true"
			},
			"apply": {
				"model": "betterend:pattern/%block%/%block%_side",
				"uvlock": true,
				"y": 180
			}
		},
		{
			"when": {
				"west": "true"
			},
			"apply": {
				"model": "betterend:pattern/%block%/%block%_side",
				"uvlock": true,
				"y": 270
			}
		}
	]
}